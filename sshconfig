# account for the autopass repo
Host github.com-autopass
    HostName github.com
    User foo
#    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa

# account for the miscellaneous repo
Host github.com-miscellaneous
    HostName github.com
    User foo
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa.github.foo.miscellaneous
Host *.domain2.com
	CheckHostIP no
	UserKnownHostsFile=/dev/null
	StrictHostKeyChecking no 
        ProxyCommand  ssh -o CheckHostIP=no -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -W $(getent hosts %h | awk '{print $1;exit}'):%p username@1.1.1.1
Host *.domain1.com
	CheckHostIP no
	UserKnownHostsFile=/dev/null
	StrictHostKeyChecking no 
        ProxyCommand  ssh -o CheckHostIP=no -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -W $(getent hosts %h | awk '{print $1;exit}'):%p username@1.1.1.1
Host *.domain.com
	CheckHostIP no
	UserKnownHostsFile=/dev/null
	StrictHostKeyChecking no 
        ProxyCommand  ssh -o CheckHostIP=no -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -W $(getent hosts %h | awk '{print $1;exit}'):%p username@1.1.1.1
